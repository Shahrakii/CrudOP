{
    "name": "shahrakii/crudly",
    "description": "Automatic, intelligent CRUD generator for Laravel 12. Generate controllers, models, migrations, and views in seconds.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shahrak",
            "email": "shahrak@example.com",
            "homepage": "https://github.com/Shahrakii"
        }
    ],
    "keywords": [
        "laravel",
        "crud",
        "generator",
        "artisan",
        "scaffold",
        "automation"
    ],
    "homepage": "https://github.com/Shahrakii/Crudly",
    "support": {
        "issues": "https://github.com/Shahrakii/Crudly/issues",
        "source": "https://github.com/Shahrakii/Crudly"
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/filesystem": "^12.0",
        "illuminate/console": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "laravel/framework": "^12.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Shahrakii\\Crudly\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shahrakii\\Crudly\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shahrakii\\Crudly\\CrudlyServiceProvider"
            ],
            "aliases": {
                "Crudly": "Shahrakii\\Crudly\\Facades\\Crudly"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
